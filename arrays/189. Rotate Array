class Solution {
public:
    void rotate(vector<int>& a, int k) {
        int n=(a.size());
        k=k% n;
        vector <int > b;
        for(int i= n-k ; i < n;i++) b.push_back(a[i]);
        int i=n-k-1 , j =n-1;
        while(i>=0){
            a[j]=a[i];
            i--;j--;
        }
        for(int i=0;i<k;i++){
            a[i]=b[i];
        }
    }
};
